{
  "name": "brews-api",
  "version": "1.0.0",
  "description": "brews app api service",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "concurrently": "5.3.0",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "mongoose": "5.11.7",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
